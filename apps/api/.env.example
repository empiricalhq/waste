# Supabase (PostgreSQL connection)
# Get this from your Supabase project settings -> Database -> Connection string -> URI
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@[YOUR-HOST]:5432/postgres"

# better-auth
# A long, random string used to sign session cookies
# Generate one with: openssl rand -hex 32
AUTH_SECRET="your_super_secret_string_of_at_least_32_characters"

# The public URL of this API. Used for redirects in OAuth flows.
AUTH_ORIGIN="http://localhost:4000"
PORT=4000
